    "scripts": {
        "apiServer": "node ./dist.server/server.bundle.js",
        "build": "cross-env BABEL_ENV=production webpack -p --config=webpack.config.prod.js",
        "build:all": "npm run build && npm run build:apiServer && npm run build:jsdoc",
        "build:apiServer": "cross-env BABEL_ENV=production webpack -p --config=webpack.config.server.js",
        "build:jsdoc": "echo \"Generating jsdoc ...\" && jsdoc -c ./jsdoc.json -R ./README.md -r ./src/ -d ./jsdoc/",
        "clean": "rimraf ./dist",
        "clean:all": "npm run clean && npm run clean:apiServer && npm run clean:jsdoc",
        "clean:apiServer": "rimraf ./dist.server",
        "clean:jsdoc": "rimraf ./jsdoc",
        "clean:modules": "rimraf ./node_modules",
        "devApiServer": "nodemon --watch ./src/ ./src/server/index.js --exec \"cross-env BABEL_ENV=commonjs babel-node --inspect\"",
        "jsdoc": "npm run clean:jsdoc && npm run build:jsdoc",
        "lint": "eslint ./src/",
        "prepublishOnly": "npm run test && npm run build:all",
        "start": "webpack-dev-server --no-inline --config=webpack.config.dev.js",
        "test": "npm run lint && echo \"Warning: no tests specified (yet)\""
    },
    "version": "0.0.1"
