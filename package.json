    },
    "scripts": {
        "apiServer": "node ./dist.server/server.bundle.js",
        "build": "npm run build:client && npm run build:host",
        "build:all": "npm run build && npm run build:apiServer && npm run build:lib && npm run build:jsdoc",
        "build:apiServer": "cross-env BABEL_ENV=prodserver webpack -p --config=webpack/webpack.config.server.js",
        "build:client": "cross-env BABEL_ENV=production webpack -p --config=webpack/webpack.config.client.prod.js",
        "build:host": "cross-env BABEL_ENV=production webpack -p --config=webpack/webpack.config.host.prod.js",
        "build:jsdoc": "echo \"Generating jsdoc ...\" && jsdoc -c ./jsdoc.json -R ./README.md -r ./src/ -d ./jsdoc/",
        "build:lib": "cross-env BABEL_ENV=prodlib webpack -p --config=webpack/webpack.config.lib.js",
        "clean": "npm run clean:client && npm run clean:host",
        "clean:all": "npm run clean && npm run clean:apiServer && npm run clean:jsdoc && npm run clean:lib",
        "clean:apiServer": "rimraf ./dist.server",
        "clean:client": "rimraf ./dist.client",
        "clean:host": "rimraf ./dist.host",
        "clean:jsdoc": "rimraf ./jsdoc",
        "clean:lib": "rimraf ./dist.lib",
        "clean:modules": "rimraf package-lock.json && rimraf node_modules",
        "devApiServer": "nodemon -e js,sql --watch ./src/ ./src/server/index.js --exec \"cross-env BABEL_ENV=devserver babel-node --inspect\"",
        "jsdoc": "npm run clean:jsdoc && npm run build:jsdoc",
        "lint": "eslint ./src/",
        "prepublishOnly": "npm run test && npm run build:all",
        "start": "webpack-dev-server --no-inline --config=webpack/webpack.config.dev.js",
        "test": "npm run lint && echo \"Warning: no tests specified (yet)\""
    },
    "version": "0.1.0"
}
