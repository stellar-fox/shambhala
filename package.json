        "clean:jsdoc": "rimraf ./jsdoc",
        "clean:modules": "rimraf ./node_modules",
        "devApiServer": "nodemon --watch ./backend.src/ ./backend.src/server.js --exec \"cross-env BABEL_ENV=commonjs babel-node --inspect\"",
        "jsdoc": "npm run clean:jsdoc && npm run buildJsdoc",
        "buildJsdoc": "jsdoc -c ./jsdoc.json -R ./README.md ./src/ -d ./jsdoc/",
        "lint": "eslint ./src/",
        "prepublishOnly": "npm run test && npm run doc",
        "start": "webpack-dev-server --config=webpack.config.dev.js",
